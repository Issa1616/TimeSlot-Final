{
  "expo": {
    "name": "TimeSlot",
    "slug": "timeslot",
    "scheme": "timeslot",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
     "EXPO_PUBLIC_API_URL": "http://192.168.12.197:4000",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#0f3a46"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#0f3a46"
      }
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-asset",
      "expo-secure-store"
    ]
  }
}